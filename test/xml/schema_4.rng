<grammar xmlns="http://relaxng.org/ns/structure/1.0">
    <start>
        <ref name="article"/>
    </start>
    <define name="content">
        <element name="content">
            <group>

                <optional>
                    <ref name="title"/>
                </optional>
                <optional>
                    <ref name="subtitle"/>
                </optional>
                <optional>
                    <ref name="text"/>
                </optional>
                <optional>
                    <ref name="reference-defs"/>
                </optional>
                <optional>
                    <ref name="strapline"/>
                </optional>
            </group>
        </element>
    </define>
    <define name="app">
        <element name="app">
            <group>
                <optional>
                    <ref name="teaser"/>
                </optional>
                <optional>
                    <ref name="key-visual"/>
                </optional>
                <optional>
                    <ref name="headline"/>
                </optional>
            </group>
        </element>
    </define>
    <define name="website">
        <element name="website">
            <group>
                <optional>
                    <ref name="teaser"/>
                </optional>
                <optional>
                    <ref name="key-visual"/>
                </optional>
            </group>
        </element>
    </define>
    <define name="print">
        <element name="print">
            <group>
                <optional>
                    <ref name="teaser"/>
                </optional>
                <optional>
                    <ref name="key-visual"/>
                </optional>
                <optional>
                    <ref name="images"/>
                </optional>
            </group>
        </element>
    </define>
    <define name="strapline">
        <element name="strapline">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="title">
        <element name="title">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="subtitle">
        <element name="subtitle">
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="text">
        <element name="text">
            <group>
                <optional>
                    <ref name="intro"/>
                </optional>
                <zeroOrMore>
                    <choice>
                        <ref name="paragraph"/>
                        <ref name="new-page"/>
                        <ref name="anchor"/>
                        <ref name="image-box"/>
                        <ref name="video-box"/>
                        <ref name="audio-box"/>
                        <ref name="slideshow-box"/>
                        <ref name="image-reveal-box"/>
                        <ref name="turntable-box"/>
                        <ref name="interactive-element-box"/>
                        <ref name="x-external-content-box"/>
                        <ref name="subheadline-1"/>
                        <ref name="subheadline-2"/>
                        <ref name="subheadline-3"/>
                        <ref name="enumeration"/>
                        <ref name="bullet-list"/>
                        <ref name="conversation"/>
                        <ref name="group"/>
                        <ref name="callout"/>
                        <ref name="pre"/>
                        <ref name="hdivider"/>
                        <ref name="callout-box"/>
                        <ref name="table"/>
                        <ref name="article"/>
                    </choice>
                </zeroOrMore>
            </group>
        </element>
    </define>
    <define name="reference-defs">
        <element name="reference-defs">
            <zeroOrMore>
                <ref name="reference-def"/>
            </zeroOrMore>
        </element>
    </define>
    <define name="reference-def">
        <element name="reference-def">
            <ref name="attlist.reference-def"/>
            <ref name="reference-description"/>
        </element>
    </define>
    <define name="attlist.reference-def">
        <optional>
            <attribute name="key"/>
        </optional>
        <optional>
            <attribute name="type">
                <choice>
                    <value>footnote</value>
                    <value>literature</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="style">
                <choice>
                    <value>style-1</value>
                    <value>style-2</value>
                    <value>style-3</value>
                </choice>
            </attribute>
        </optional>
    </define>
    <define name="teaser">
        <element name="teaser">
            <group>

                <optional>
                    <ref name="title"/>
                </optional>
                <optional>
                    <ref name="teaser-text"/>
                </optional>
                <optional>
                    <ref name="image"/>
                </optional>
                <optional>
                    <ref name="strapline"/>
                </optional>
            </group>
        </element>
    </define>
    <define name="teaser-text">
        <element name="teaser-text">

            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="link"/>
                    <ref name="marker"/>
                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="key-visual">
        <element name="key-visual">

            <ref name="attlist.key-visual"/>
            <group>
                <optional>
                    <choice>
                        <ref name="image"/>
                        <ref name="video"/>
                        <ref name="audio"/>
                        <ref name="slideshow"/>
                        <ref name="interactive-element"/>
                        <ref name="image-reveal"/>
                        <ref name="turntable"/>
                        <ref name="image-range"/>
                        <ref name="layout"/>
                        <ref name="gallery"/>
                    </choice>
                </optional>
                <optional>
                    <ref name="poster"/>
                </optional>
                <optional>
                    <ref name="caption"/>
                </optional>
                <optional>
                    <ref name="key-visual-text-options"/>
                </optional>
            </group>
        </element>
    </define>
    <define name="attlist.key-visual">
        <optional>
            <attribute name="type" >
                <choice>
                    <value>none</value>
                    <value>image</value>
                    <value>video</value>
                    <value>audio</value>
                    <value>slideshow</value>
                    <value>interactive-element</value>
                    <value>image-reveal</value>
                    <value>turntable</value>
                    <value>image-range</value>
                    <value>layout</value>
                    <value>gallery</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="display">
                <choice>
                    <value>inline</value>
                    <value>fullscreen</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="distance-top">
                <choice>
                    <value>true</value>
                    <value>false</value>
                </choice>
            </attribute>
        </optional>
    </define>
    <define name="key-visual-text-options">
        <element name="key-visual-text-options">

            <ref name="attlist.key-visual-text-options"/>
        </element>
    </define>
    <define name="attlist.key-visual-text-options">
        <optional>
            <attribute name="position" >
                <choice>
                    <value>none</value>
                    <value>on-top</value>
                    <value>hotspot</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="width"/>
        </optional>
        <optional>
            <attribute name="position-origin" >
                <choice>
                    <value>top-left</value>
                    <value>top-right</value>
                    <value>bottom-left</value>
                    <value>bottom-right</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="x-position"/>
        </optional>
        <optional>
            <attribute name="y-position"/>
        </optional>
        <optional>
            <attribute name="background-color"/>
        </optional>
        <optional>
            <attribute name="background-opacity"/>
        </optional>
        <optional>
            <attribute name="text-color"/>
        </optional>
        <optional>
            <attribute name="text-opacity"/>
        </optional>
    </define>
    <define name="headline">
        <element name="headline">

            <ref name="attlist.headline"/>
        </element>
    </define>
    <define name="attlist.headline">
        <optional>
            <attribute name="position">
                <choice>
                    <value>none</value>
                    <value>below</value>
                    <value>above</value>
                    <value>on-top</value>
                </choice>
            </attribute>
        </optional>
        <attribute name="width"/>
        <optional>
            <attribute name="position-origin">
                <choice>
                    <value>top-left</value>
                    <value>top-right</value>
                    <value>bottom-left</value>
                    <value>bottom-right</value>
                </choice>
            </attribute>
        </optional>
        <attribute name="x-position"/>
        <optional>
            <attribute name="y-position"/>
        </optional>
        <optional>
            <attribute name="background-color"/>
        </optional>
        <optional>
            <attribute name="background-opacity"/>
        </optional>
        <optional>
            <attribute name="text-color"/>
        </optional>
        <optional>
            <attribute name="text-opacity"/>
        </optional>
    </define>
    <define name="images">
        <element name="images">

            <zeroOrMore>
                <ref name="image-box"/>
            </zeroOrMore>
        </element>
    </define>
    <define name="paragraph">
        <element name="paragraph">

            <ref name="attlist.paragraph"/>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="link"/>
                    <ref name="marker"/>
                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                    <ref name="reference"/>
                    <ref name="footnote"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="attlist.paragraph">
        <optional>
            <attribute name="style">
                <choice>
                    <value>style-1</value>
                    <value>style-2</value>
                    <value>style-3</value>
                    <value>style-4</value>
                    <value>style-5</value>
                </choice>
            </attribute>
        </optional>
    </define>
    <define name="subheadline-1">
        <element name="subheadline-1">

            <ref name="attlist.subheadline-1"/>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                    <ref name="reference"/>
                    <ref name="footnote"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="attlist.subheadline-1">
        <optional>
            <attribute name="style">
                <choice>
                    <value>style-1</value>
