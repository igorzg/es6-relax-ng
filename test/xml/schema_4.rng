    <define name="image-box">
        <element name="image-box">
            <ref name="attlist.image-box"/>
            <group>
                <ref name="image"/>
                <optional>
                    <ref name="caption"/>
                </optional>
            </group>
        </element>
    </define>
    <define name="attlist.image-box">
        <optional>
            <attribute name="placement-category"/>
        </optional>
        <optional>
            <attribute name="sorting"/>
        </optional>
        <optional>
            <attribute name="zoom">
                <choice>
                    <value>true</value>
                    <value>false</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="link"/>
        </optional>
        <optional>
            <attribute name="align">
                <choice>
                    <value>left</value>
                    <value>right</value>
                    <value>center</value>
                    <value>sidebar</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="style">
                <choice>
                    <value>style-1</value>
                    <value>style-2</value>
                    <value>style-3</value>
                </choice>
            </attribute>
        </optional>
    </define>
    <define name="image">
        <element name="image">
            <ref name="attlist.image"/>
        </element>
    </define>
    <define name="attlist.image">
        <optional>
            <attribute name="aspect-ratio">
                <choice>
                    <value>16-9</value>
                    <value>4-3</value>
                    <value>3-4</value>
                    <value>1-1</value>
                    <value>flexible</value>
                </choice>
            </attribute>
        </optional>
        <optional>
            <attribute name="alt-text"/>
        </optional>
        <optional>
            <attribute name="seo-title"/>
        </optional>
