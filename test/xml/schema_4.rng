            </attribute>
        </optional>
    </define>
    <define name="enumeration">
        <element name="enumeration">

            <ref name="attlist.enumeration"/>
            <oneOrMore>
                <ref name="item"/>
            </oneOrMore>
        </element>
    </define>
    <define name="attlist.enumeration">
        <optional>
            <attribute name="style">
                <choice>
                    <value>style-1</value>
                    <value>style-2</value>
                    <value>style-3</value>
                </choice>
            </attribute>
        </optional>
    </define>
    <define name="item">
        <element name="item">

            <oneOrMore>
                <choice>
                    <ref name="paragraph"/>
                    <ref name="anchor"/>
                    <ref name="image-box"/>
                    <ref name="enumeration"/>
                    <ref name="bullet-list"/>
                    <ref name="pre"/>
                    <ref name="article"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="pre">
        <element name="pre">

            <ref name="attlist.pre"/>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="link"/>
                    <ref name="marker"/>
                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                    <ref name="reference"/>
                    <ref name="footnote"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="attlist.pre">
        <attribute name="code-type">
            <choice>
                <value>application/javascript</value>
                <value>application/x-freemarker</value>
                <value>application/x-java</value>
                <value>application/x-perl</value>
                <value>application/x-php</value>
                <value>application/x-python</value>
                <value>application/x-ruby</value>
                <value>application/xhtml+xml</value>
                <value>application/xslt+xml</value>
                <value>image/svg+xml</value>
                <value>text/css</value>
                <value>text/html</value>
                <value>text/plain</value>
                <value>text/x-xpress</value>
                <value>text/xml</value>
            </choice>
        </attribute>
        <optional>
            <attribute name="style">
                <choice>
                    <value>style-1</value>
                    <value>style-2</value>
                    <value>style-3</value>
                </choice>
            </attribute>
        </optional>
    </define>
    <define name="group">
        <element name="group">

            <ref name="attlist.group"/>
            <oneOrMore>
                <choice>
                    <ref name="paragraph"/>
                    <ref name="image-box"/>
                    <ref name="video-box"/>
                    <ref name="audio-box"/>
                    <ref name="slideshow-box"/>
                    <ref name="image-reveal-box"/>
                    <ref name="turntable-box"/>
                    <ref name="interactive-element-box"/>
                    <ref name="x-external-content-box"/>
                    <ref name="subheadline-1"/>
                    <ref name="subheadline-2"/>
                    <ref name="subheadline-3"/>
                    <ref name="enumeration"/>
                    <ref name="bullet-list"/>
                    <ref name="conversation"/>
                    <ref name="hdivider"/>
                    <ref name="additional-content"/>
                    <ref name="callout"/>
                    <ref name="table"/>
                    <ref name="pre"/>
                    <ref name="article"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="attlist.group">
        <optional>
            <attribute name="style">
                <choice>
                    <value>style-1</value>
                    <value>style-2</value>
                    <value>style-3</value>
                </choice>
            </attribute>
        </optional>
    </define>
    <define name="intro">
        <element name="intro">
            <ref name="attlist.intro"/>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="link"/>
                    <ref name="marker"/>
                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                    <ref name="reference"/>
                    <ref name="footnote"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="attlist.intro">
        <optional>
            <attribute name="style">
                <choice>
