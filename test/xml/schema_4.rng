                    <ref name="bold"/>
                    <ref name="italic"/>
                    <ref name="bold-italic"/>
                    <ref name="underline"/>
                    <ref name="sup"/>
                    <ref name="sub"/>
                    <ref name="reference"/>
                    <ref name="footnote"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="attlist.subheadline-2">
        <optional>
            <attribute name="style">
                <choice>
                    <value>style-1</value>
                    <value>style-2</value>
                    <value>style-3</value>
                </choice>
            </attribute>
        </optional>
    </define>
    <define name="subheadline-3">
        <element name="subheadline-3">

            <ref name="attlist.subheadline-3"/>
            <oneOrMore>
                <choice>
                    <text/>
                    <ref name="bold"/>
