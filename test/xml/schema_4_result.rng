    <define name="content">
        <element>
            <name ns="">content</name>
            <group>
                <group>
                    <group>
                        <group>
                            <choice>
                                <ref name="title"/>
                                <empty/>
                            </choice>
                            <choice>
                                <ref name="subtitle"/>
                                <empty/>
