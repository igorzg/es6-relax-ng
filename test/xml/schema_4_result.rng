    <define name="reference-defs">
        <element>
            <name ns="">reference-defs</name>
            <choice>
                <oneOrMore>
                    <ref name="reference-def"/>
                </oneOrMore>
                <empty/>
            </choice>
        </element>
    </define>
    <define name="reference-def">
        <element>
            <name ns="">reference-def</name>
            <group>
                <group>
                    <group>
                        <choice>
                            <attribute>
                                <name ns="">key</name>
                                <text/>
                            </attribute>
                            <empty/>
                        </choice>
                        <choice>
                            <attribute>
                                <name ns="">type</name>
                                <choice>
                                    <value type="token" ns="">footnote</value>
                                    <value type="token" ns="">literature</value>
                                </choice>
                            </attribute>
                            <empty/>
                        </choice>
                    </group>
                    <choice>
                        <attribute>
                            <name ns="">style</name>
                            <choice>
