                                    <value type="token" ns="">style-1</value>
                                    <value type="token" ns="">style-2</value>
                                </choice>
                                <value type="token" ns="">style-3</value>
                            </choice>
                        </attribute>
                        <empty/>
                    </choice>
                </group>
                <ref name="reference-description"/>
            </group>
        </element>
    </define>
